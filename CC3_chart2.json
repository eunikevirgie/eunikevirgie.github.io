{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "Gross Domestic Product: Year on Year growth CVM SA %",

  "title" : "Gross Domestic Product: Year on Year growth CVM SA %",

  "width" : 600,
  "height" : 250,
  "padding" : 5,

  "config": {
    "axis": {
      "grid": false 
    }
  },

  "data": {
    "url": "https://raw.githubusercontent.com/eunikevirgie/eunikevirgie.github.io/refs/heads/main/UK%20GDP%20Annual%20Growth.csv",
    "format": {"type": "dsv", "delimiter": ","}
  },

  "mark": {
    "type": "line",
    "color": "#36b7b4",
    "point": {"color": "#36b7b4"} 
  },

  "encoding": {
    "x": {"field": "Date", "type": "temporal"},
    "y":{"field": "Value", "type": "quantitative"},
    "tooltip": [
      {"field": "Date", "type": "temporal", "title": "Date"},
      {"field": "Value", "type": "quantitative", "title": "GDP YoY (%)"}
    ]
  }

}